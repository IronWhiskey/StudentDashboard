
<h1>Editing <%=@dojo.branch%></h1>

<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>

<form action="/dojos/<%=@dojo['id']%>" method="post">
    <input type="hidden" name="_method" value="patch">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    
    <table>
        <tr>
            <td>Branch:</th>
            <td><input type="text" name="dojo[branch]" value="<%=@dojo.branch%>"/></th> 
        </tr>
        <tr>
            <td>Street:</th>
            <td><input type="text" name="dojo[street]" value="<%=@dojo.street%>"/></th> 
        </tr>
        <tr>
            <td>City:</th>
            <td><input type="text" name="dojo[city]" value="<%=@dojo.city%>"/></th> 
        </tr>
        <tr>
            <td>State:</th>
            <td><input type="text" name="dojo[state]" value="<%=@dojo.state%>"/></th> 
        </tr>
    </table>
    <input type='submit' value='Update'>    
</form>
<p><%= link_to "Home", root_path %> | <%= link_to "Edit", editDojo_path(@dojo.id) %> </p>